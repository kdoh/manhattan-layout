{
  "name": "manhattan-layout",
  "version": "0.0.0-semantic-release",
  "description": "Layout utilities for React built on top of Manhattan",
  "scripts": {
    "commit": "git-cz",
    "start": "node server.js",
    "test": "NODE_ENV=test mocha --require babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "prepublish": "mkdir -p dist && babel src --out-dir dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kdoh/manhattan-layout"
  },
  "author": "Kyle Doherty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kdoh/manhattan-layout/issues"
  },
  "homepage": "https://github.com/kdoh/manhattan-layout",
  "peerDependencies": {
    "react": "^0.14.*"
  },
  "dependencies": {
    "react-dom": "^0.14.7",
    "react-manhattan": "^0.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "commitizen": "^2.7.3",
    "cz-conventional-changelog": "^1.1.5",
    "enzyme": "^2.2.0",
    "expect": "^1.15.2",
    "mocha": "^2.4.5",
    "portfinder": "^0.4.0",
    "react-addons-test-utils": "^0.14.7",
    "semantic-release": "^4.3.5",
    "standard": "^6.0.8",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
